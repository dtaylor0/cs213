<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="SPage" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #14242B;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ShowController">
    <children>
      <Button mnemonicParsing="false" onAction="#goHome" text="Home">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Show Accounts" underline="true">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </Text>
        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Print Options">
            <font>
                <Font size="14.0" />
            </font>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </Text>
        <RadioButton fx:id="unsorted" mnemonicParsing="false" text="Unsorted" textFill="WHITE">
            <VBox.margin>
                <Insets left="10.0" top="10.0" />
            </VBox.margin>
         <toggleGroup>
            <ToggleGroup fx:id="printOptions" />
         </toggleGroup></RadioButton>
        <RadioButton fx:id="dateOpened" mnemonicParsing="false" text="Sorted by Date Opened" textFill="WHITE" toggleGroup="$printOptions">
            <VBox.margin>
                <Insets left="10.0" top="10.0" />
            </VBox.margin>
        </RadioButton>
        <RadioButton fx:id="lastName" mnemonicParsing="false" text="Sorted by Last Name" textFill="WHITE" toggleGroup="$printOptions">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" top="10.0" />
            </VBox.margin>
        </RadioButton>
      <Button fx:id="printButton" mnemonicParsing="false" onAction="#printAccounts" text="Print">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <TextArea fx:id="textArea" prefHeight="335.0" prefWidth="880.0" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
