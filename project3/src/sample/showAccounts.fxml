<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="OPage" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #14242B;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ShowController">
    <children>
        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Show Accounts" underline="true">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </Text>
        <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Print Options">
            <font>
                <Font size="14.0" />
            </font>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </Text>
        <RadioButton fx:id="checkingBtn" mnemonicParsing="false" text="Unsorted" textFill="WHITE">
            <VBox.margin>
                <Insets left="10.0" top="10.0" />
            </VBox.margin>
         <toggleGroup>
            <ToggleGroup fx:id="printOptions" />
         </toggleGroup></RadioButton>
        <RadioButton fx:id="savingsBtn" mnemonicParsing="false" text="Sorted by Date Opened" textFill="WHITE" toggleGroup="$printOptions">
            <VBox.margin>
                <Insets left="10.0" top="10.0" />
            </VBox.margin>
        </RadioButton>
        <RadioButton fx:id="MMBtn" mnemonicParsing="false" text="Sorted by Last Name" textFill="WHITE" toggleGroup="$printOptions">
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" top="10.0" />
            </VBox.margin>
        </RadioButton>
      <Button mnemonicParsing="false" text="Print">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
